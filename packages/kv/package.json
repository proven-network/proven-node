{
  "name": "@proven-network/kv",
  "description": "Access key-value data inside Proven runtime",
  "version": "0.0.8",
  "homepage": "https://proven.network",
  "main": "dist/kv.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/kv.mjs"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./dist/kv.umd.js"
      }
    }
  },
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "tsc && vite build && vite build --config vite-single-file.config.ts"
  },
  "dependencies": {
    "@proven-network/crypto": "0.0.4"
  },
  "devDependencies": {
    "@babel/preset-env": "7.26.0",
    "@babel/preset-typescript": "7.26.0",
    "@types/elliptic": "6.4.18",
    "babel-preset-vite": "1.1.3",
    "elliptic": "6.6.1",
    "prettier": "3.4.2",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "vite": "6.0.7",
    "vite-plugin-singlefile": "2.1.0"
  }
}
